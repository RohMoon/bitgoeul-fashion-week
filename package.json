{
  "name": "bitgoeul-fashion-week",
  "version": "1.0.0",
  "description": "Bitgoeul Fashion Week 2025 - Designer Showcase Website",
  "main": "index.html",
  "scripts": {
    "dev": "live-server --port=3000 --open=index.html",
